# Appwrite Configuration
# Copy this to .env.local and fill in your actual values

# Appwrite API endpoint (e.g., https://cloud.appwrite.io/v1)
NEXT_PUBLIC_APPWRITE_ENDPOINT=

# Your Appwrite Project ID
NEXT_PUBLIC_APPWRITE_PROJECT_ID=

# Your Appwrite Database ID (e.g., "RitamBharatDB")
NEXT_PUBLIC_APPWRITE_DATABASE_ID=

# Your Appwrite Collection ID for contacts
APPWRITE_CONTACTS_COLLECTION_ID=

# Secret server-side API Key (DO NOT expose to client)
APPWRITE_API_KEY=
